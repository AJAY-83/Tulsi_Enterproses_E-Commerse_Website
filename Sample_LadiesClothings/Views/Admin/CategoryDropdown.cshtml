@model List<Category> //IEnumerable<Sample_LadiesClothings.Models.Category>
    <select class="form-select" name="Category_Id">
    <option value="">Select Category</option>
    @foreach (var category in Model)
    {
        <option value="@category.Category_Id" selected="@category.Category_Id==ViewBag.Cat_id">@category.Category_Name</option>
    }

</select>